{
  "name": "mainstream",
  "version": "1.0.0",
  "description": "Node application to filter specific tweets from twitter and serve them via websocket",
  "main": "lib/index.js",
  "author": "Matthieu Holzer",
  "license": "MIT",
  "scripts": {
    "build" : "cross-env NODE_ENV=production babel lib -d dist",
    "dev": "cross-env NODE_ENV=development nodemon lib/index.js --exec babel-node",
    "lint": "eslint lib",
    "start": "npm run build && node dist/index.js"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-eslint": "8.2.2",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-flow": "6.23.0",
    "cross-env": "5.1.3",
    "eslint": "^4.18.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-flowtype": "2.46.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-prettier": "2.6.0",
    "nodemon": "^1.15.1",
    "prettier": "1.10.2"
  },
  "dependencies": {
    "dotenv": "5.0.0",
    "node-tweet-stream": "^2.0.3",
    "uws": "9.14.0",
    "winston": "^2.4.0"
  }
}
